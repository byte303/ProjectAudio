<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_music">

    <fragment
        android:id="@+id/navigation_music"
        android:name="com.example.projectaudio.ui.music.MusicFragment"
        android:label="@string/text_music"
        tools:layout="@layout/fragment_music" />

    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.example.projectaudio.ui.favorite.FavoriteFragment"
        android:label="@string/text_favorite"
        tools:layout="@layout/fragment_favorite" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.projectaudio.ui.setting.SettingFragment"
        android:label="@string/text_setting"
        tools:layout="@layout/fragment_setting" />
</navigation>